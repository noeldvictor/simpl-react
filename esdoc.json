{
    "source": "./src",
    "destination": "./docs",
    "undocumentIdentifier": false,
    "title": "Simpl-react",
    "plugins": [
        {"name": "esdoc-es7-plugin"},
        {
            "name": "esdoc-importpath-plugin",
            "option": {
                "replaces": [
                  {"from": "^src/", "to": "lib/"},
                  {"from": ".js$", "to": ""}
                ]
            }
        }
    ]
}

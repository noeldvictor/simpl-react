{
    "source": "./src",
    "destination": "./docs",
    "undocumentIdentifier": false,
    "title": "Simpl-react",
    "experimentalProposal": {
        "classProperties": true,
        "objectRestSpread": true,
        "decorators": true
    },
    "plugins": [
        {
            "name": "esdoc-importpath-plugin",
            "option": {
                "replaces": [
                  {"from": "^src/", "to": "lib/"},
                  {"from": ".js$", "to": ""}
                ]
            }
        }
    ]
}
